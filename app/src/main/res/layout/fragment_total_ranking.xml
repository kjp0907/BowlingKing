<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    tools:context="com.example.tacademy.bowlingkingproject.TabPager.frag.TotalRankingFragment">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"

        android:gravity="center_horizontal">

        <Button
            android:text="동호회 "
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:id="@+id/clubranking"
            android:background="@drawable/round_left_white"
            android:onClick="on1" />

        <Button
            android:text="볼링장"
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:id="@+id/alleyrangking"
            android:background="@drawable/round_right_white"
            android:onClick="on2" />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/rankingfragment"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="380dp"></LinearLayout>

</LinearLayout>